<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated multiactivity -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.sirius.sample.multiactivity.presentation.MultiactivityModelWizardID"
            name="%_UI_MultiactivityModelWizard_label"
            class="org.eclipse.sirius.sample.multiactivity.presentation.MultiactivityModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MultiactivityModelFile.gif">
         <description>%_UI_MultiactivityModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated multiactivity -->
      <editor
            id="org.eclipse.sirius.sample.multiactivity.presentation.MultiactivityEditorID"
            name="%_UI_MultiactivityEditor_label"
            icon="icons/full/obj16/MultiactivityModelFile.gif"
            extensions="multiactivity"
            class="org.eclipse.sirius.sample.multiactivity.presentation.MultiactivityEditor"
            contributorClass="org.eclipse.sirius.sample.multiactivity.presentation.MultiactivityActionBarContributor">
      </editor>
   </extension>

</plugin>
